{
  "name": "@becomes/svelte-router",
  "version": "1.0.1",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "build:src": "tsc -p tsconfig.build.json",
    "validate": "svelte-check",
    "bundle": "node tools.js --bundle",
    "link": "node tools.js --link",
    "unlink": "node tools.js --unlink",
    "lint": "eslint ./src --ext .ts",
    "check": "svelte-check --fail-on-warnings --fail-on-hints --workspace src"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/fs-extra": "^9.0.6",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "eslint-plugin-svelte3": "^3.0.0",
    "fs-extra": "^9.1.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^1.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "uuid": "^8.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/becomesco/svelte-router.git"
  },
  "keywords": [],
